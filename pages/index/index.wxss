  margin-bottom: 16rpx; 
  font-family: "微软雅黑";
  font-size: 22rpx;
  color: #fefeff;
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
  background-position: center center;
  background-size: cover;
 background-repeat: no-repeat;
}
.special-fish navigator{
  width: 100%;
  height: 100%;
}
.price{
  display: flex;
  align-items: center;
}
.sign{
   font-size: 20rpx;
