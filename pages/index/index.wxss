/**index.wxss**/

swiper{
  height: 330rpx;
}
swiper navigator{
  width: 100%;
  height: 100%;
}
swiper image{
  width:100%;
  height: 100%;

}
.swiper-box .wx-swiper-dots.wx-swiper-dots-horizontal{
     margin-bottom: 2rpx;
}
.swiper-box .wx-swiper-dot{
    width:40rpx;
    display: inline-flex;
    height: 10rpx;
    margin-left: 20rpx;
    justify-content:space-between;
}
.swiper-box .wx-swiper-dot::before{
    content: '';
    flex-grow: 1; 
    background: rgba(255,255,255,0.8);
    border-radius: 8rpx
}
.swiper-box .wx-swiper-dot-active::before{
    background:rgba(244,0,0,0.8);   
}
.options{
  width:100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
  background: #fff;
  margin-bottom: 16rpx;
  margin-top: 16rpx;
  
}
.options-item{
  width:25%;
 height: 175rpx;
 display: flex;
 flex-direction: column;
 align-items: center;
 justify-content: center;
}

.options-pic{
  width:80rpx;
  height: 80rpx;
  display: inline-block;
}
.options-name{
  font-family: 微软雅黑;
  font-size: 24rpx;
  text-align: center;
  color: #232324;
}
.special{
  width:100%;
  background: #fff;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.special .title{
  height: 62rpx;
  font-size: 26rpx;
  font-weight: 600;
  text-align: center;
  color: #37383b;
