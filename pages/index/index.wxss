  width: 100%;
}
.details--item{
  background: #fff;
  margin-bottom: 15rpx;
  padding-bottom: 14rpx;
    box-sizing: border-box;
}
.details--item:nth-child(odd){
  float:left;
}
.details--item:nth-child(even){
  float:right;
}

.details--item image{
  width: 350rpx;
  height: 350rpx;
  margin-bottom: 14rpx;
}
.details--item .name{
   font-family: "微软雅黑";
